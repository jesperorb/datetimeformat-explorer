		"@sveltejs/kit": "1.25.0",
		"@testing-library/svelte": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"axe-playwright": "^1.2.3",
		"eslint": "^8.50.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.2",
		"highlight.js": "^11.8.0",
