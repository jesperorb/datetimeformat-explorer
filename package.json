		"svelte-check": "^3.6.3",
		"svelte-eslint-parser": "^0.33.1",
		"svelte-highlight": "^7.5.0",
		"svelte-preprocess": "^5.1.3",
		"tslib": "^2.6.2",
		"typescript": "^5.4.2",
		"vite": "^5.0.12",
		"vitest": "^1.2.2"
	}
